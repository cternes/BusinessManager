-- Seminarstamm
DROP TABLE IF EXISTS seminarstamm;
CREATE TABLE seminarstamm (
  SEKey int NOT NULL,
  SEBez varchar(255) DEFAULT NULL,
  SEDatum date DEFAULT NULL,
  SEOrt varchar(255) DEFAULT NULL,
  SEPreis int DEFAULT NULL,
  SEBeginn varchar(255) DEFAULT NULL,
  SEEnde varchar(255) DEFAULT NULL,
  SETeilnehmer int DEFAULT NULL,
  SEKursleiter varchar(255) DEFAULT NULL,
  SEZeit varchar(255) DEFAULT NULL,
  SEGK BOOLEAN DEFAULT NULL,
  SEWB BOOLEAN DEFAULT NULL,
  SEEX BOOLEAN DEFAULT NULL,
  SEAP BOOLEAN DEFAULT NULL,
  SEVeranstalter varchar(255) DEFAULT NULL,
  SEJN BOOLEAN DEFAULT NULL,
  SEVerpflegung int DEFAULT NULL,
  PRIMARY KEY (SEKey)
);

/*************GENERAL*************/
* {
	margin: 0;
	padding: 0;
}

p {
	margin: 5px 0 10px 0;
}

body,html {
	font-family: Verdana, Arial, Helvetica;
	font-size: 11px;
	color: #00467f;
	height: 100%;
}

body {
	background: url("#{resource['images:bg-content.jpg']}") repeat-y scroll
		center top #FFFFFF;
}

h1,h2,h3,h4,h5,h6 {
	font-weight: normal;
}

h2 {
	font-size: 15px;
}

h3 {
	font-size: 12px;
	font-weight: bold;
	padding-top: 25px;
	padding-bottom: 15px;
}

h4 {
	font-size: 12px;
}

.contentheading,.componentheading {
	font-size: 15px;
	padding-bottom: 20px;
}

.spacer {
	width: 100%;
	height: 30px;
}

.inner {
	padding: 0 35px;
}

a {
	color: #00467f;
	text-decoration: none;
}

#main a,#text a {
	color: #666;
}

.stickAtRightBorder { /* Sticks at the right border */
	text-align: right;
}


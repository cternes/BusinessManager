<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:sec="http://www.springframework.org/security/facelets/tags" 
	>

<!-- Needed for Chrome & Safari -->
<f:view contentType="text/html">
	<h:head>
		<title>Vqf Collaboration Platform</title>
		<meta content='text/html; charset=UTF-8' http-equiv="Content-Type"/>
		<h:outputStylesheet name="css/screen.css" />
		<h:outputStylesheet name="css/theme.css" />
		 <script type="text/javascript">
      //<![CDATA[
        PrimeFaces.locales['de'] = {
        		closeText: 'Schließen',
                prevText: 'Zurück',
                nextText: 'Weiter',
                currentText: 'Heute',
                monthNames: ['Januar', 'Februar', 'März', 'April', 'Mai', 'Juni', 'Juli', 'August', 'September', 'Oktober', 'November', 'Dezember' ],
                monthNamesShort: ['Jan', 'Feb', 'Mär', 'Apr', 'Mai', 'Jun', 'Jul', 'Aug', 'Sep', 'Okt', 'Nov', 'Dez' ],
                dayNames: ['Sonntag', 'Montag', 'Dienstag', 'Mittwoch', 'Donnerstag', 'Freitag', 'Samstag'],
                dayNamesShort: ['Son', 'Mon', 'Die', 'Mit', 'Don', 'Fre', 'Sam'],
                dayNamesMin: ['S', 'M', 'D', 'M ', 'D', 'F ', 'S'],
                weekHeader: 'Woche',
                firstDay: 1,
                isRTL: false,
                showMonthAfterYear: false,
                yearSuffix: '',
                timeOnlyTitle: 'Uhrzeit auswählen',
                timeText: 'Zeit',
                hourText: 'Stunde',
                minuteText: 'Minute',
                secondText: 'Sekunde',
                currentText: 'Aktuelles Datum',
                ampm: false,
                month: 'Monat',
                week: 'Woche',
                day: 'Tag',
                allDayText: 'Ganzer Tag'
        };

	//]]>
        </script>
		<ui:insert name="head"></ui:insert>
	</h:head>
	<h:body>
	<div id="page-body">
		<div id="wrapper">
			<!-- HEADER -->
			<div id="header">
				<div class="inner">
					<div class="spacer"></div>
					<div id="topmenu">
						<h:form id="linkForm">
						<ul class="menu">
							<sec:isAuthenticated>
								<li>
									<h:commandLink action="#{navigationHelper.getUserSettings}" >
									#{messages.username}: #{facesContext.externalContext.userPrincipal.name}
									</h:commandLink>
								</li>
								<li>
									<a href="#{facesContext.externalContext.request.contextPath}/j_spring_security_logout">#{messages.logout}</a>
								</li>
							</sec:isAuthenticated>
						</ul>
						</h:form>
					</div>
				</div>
			</div>
				
			<div id="logo">
				<div class="inner">
					<h:form id="logoForm">
						<h:commandLink action="#{navigationHelper.getHome}" styleClass="noLinkColor"><img src="#{resource['images:vqf-logo.jpg']}" alt="VQF" title="Home" />
						</h:commandLink>
					</h:form>
				</div>
			</div>
			<div id="slogan">
				<div class="inner">
					#{messages.header_slogan}							
				</div>
			</div>
			<!-- END HEADER -->
			
			<!-- MAINTABS -->
			<div class="inner">
					<h:form id="menubarForm">
						<p:menubar autoSubmenuDisplay="true" effect="fade" effectDuration="1">
							<p:menuitem value="#{messages.maintab_sd}" action="#{navigationHelper.getSelfdeclarationHome}" rendered="#{sec:ifAnyGranted('Navigation Selbstdeklaration,Administratorrecht')}" disabled="true">
							</p:menuitem>
							<p:submenu label="#{messages.maintab_weiterbildung}" rendered="#{sec:ifAnyGranted('Navigation Aus-Weiterbildung,Administratorrecht')}" disabled="true">
								<p:menuitem value="#{messages.grundausbildung_title}" action="#{navigationHelper.getSeminarListBasic}" rendered="#{sec:ifAnyGranted('Navigation Grundausbildung,Administratorrecht')}" >
								</p:menuitem>
								<p:menuitem value="#{messages.weiterbildung_title}" action="#{navigationHelper.getSeminarListAdvanced}" rendered="#{sec:ifAnyGranted('Navigation Weiterbildung,Administratorrecht')}" >
								</p:menuitem>
								<p:menuitem value="#{messages.warteliste_title}" action="#{navigationHelper.getSeminarWaitlist}" rendered="#{sec:ifAnyGranted('Navigation Warteliste,Administratorrecht')}">
								</p:menuitem>
							</p:submenu>)  
							<p:submenu label="#{messages.maintab_mutation}" rendered="#{sec:ifAnyGranted('Navigation Stammdatenmutation,Administratorrecht')}" styleClass="ui-state-disabled">
								<!-- 
								<p:menuitem value="#{messages.mutationerstellen_title}" action="#{navigationHelper.getMutationCreate}" rendered="#{sec:ifAnyGranted('Navigation Mutation-Erstellen,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.mutationoffen_title}" action="#{navigationHelper.getMutationOffeneMutationen}" rendered="#{sec:ifAnyGranted('Navigation ListeOffenerMutationen,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.mutationkontrolle_title}" action="#{navigationHelper.getMutationControl}" rendered="#{sec:ifAnyGranted('Navigation Mutation-Kontrolle,Administratorrecht')}">
								</p:menuitem>
								 -->
							</p:submenu>
							<p:submenu label="#{messages.maintab_pruefungen}" rendered="#{sec:ifAnyGranted('Navigation Prüfungen,Administratorrecht')}" styleClass="ui-state-disabled">
								<!--  
								<p:menuitem value="#{messages.pruefungsListe_title}" action="#{navigationHelper.getAssessmentList}" rendered="#{sec:ifAnyGranted('Navigation Prüfungsliste,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.vereinbartepruefungen_title}" action="#{navigationHelper.getAssessmentAppointed}" rendered="#{sec:ifAnyGranted('Navigation Vereinbarte Prüfungen,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.terminkalender_title}" action="#{navigationHelper.getAssessmentSchedule}" rendered="#{sec:ifAnyGranted('Navigation Terminkalender,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.controlling_title}" action="#{navigationHelper.getAssessmentControlling}" rendered="#{sec:ifAnyGranted('Navigation Controlling,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.budgetierung_title}" action="#{navigationHelper.getAssessmentBudget}" rendered="#{sec:ifAnyGranted('Navigation Budgetierung,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.aktuellePruefung_title}" action="#{navigationHelper.getAssessmentCurrentAssessment}" rendered="#{sec:ifAnyGranted('Navigation Aktuelle Prüfung')}" >
								</p:menuitem>
								-->
							</p:submenu>
							<p:submenu label="#{messages.maintab_add}" rendered="#{sec:ifAnyGranted('Navigation Hinzufügen,Administratorrecht')}">
								<p:menuitem value="#{messages.personwizard_title}" action="#{navigationHelper.getAddPerson}" rendered="#{sec:ifAnyGranted('Navigation Hinzufügen Benutzer,Administratorrecht')}" />
								<p:menuitem value="#{messages.rolemanagement_role_add}" action="#{navigationHelper.getAdminRoleUpdate}" rendered="#{sec:ifAnyGranted('Rollen hinzufügen,Administratorrecht')}" />
								<p:menuitem value="#{messages.vqfcommunitymanagement_add}" action="#{navigationHelper.getAdminMemberCommunityUpdate}" rendered="#{sec:ifAnyGranted('Navigation Mitgliedergemeinschaft hinzufügen,Administratorrecht')}" />
							</p:submenu>
							<p:submenu label="#{messages.maintab_admin}" rendered="#{sec:ifAnyGranted('Navigation Administration,Administratorrecht')}">
								<p:menuitem value="#{messages.prueferverwaltung_title}" action="#{navigationHelper.getAdminAuditorManagement}" rendered="#{sec:ifAnyGranted('Navigation Prüfer,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.mitgliederzusatzdatenverwaltung_title}" action="#{navigationHelper.getAdminMemberManagement}" rendered="#{sec:ifAnyGranted('Navigation Mitgliederzusatzdaten,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.mitgliedergemeinschaftenverwaltung_title}" action="#{navigationHelper.getAdminMemberCommunityManagement}" rendered="#{sec:ifAnyGranted('Navigation Mitgliedergemeinschaften,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.usermanagement_title}" action="#{navigationHelper.getAdminUserManagement}" rendered="#{sec:ifAnyGranted('Navigation Benutzer,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.rolemanagement_title}" action="#{navigationHelper.getAdminRoleManagement}" rendered="#{sec:ifAnyGranted('Navigation Rollen,Administratorrecht')}">
								</p:menuitem>
								<p:menuitem value="#{messages.globalsettings_title}" action="#{navigationHelper.getAdminGlobalSettings}" rendered="#{sec:ifAnyGranted('Administratorrecht')}">
								</p:menuitem>
								
							</p:submenu>
						</p:menubar>
					</h:form>
			</div>
			<!-- END MAINTABS -->
		
			<h:panelGroup layout="block">
				<h:panelGroup id="main" layout="block">
					<div class="inner">
						<h:panelGroup id="content" layout="block">
							<div class="spacer"></div>
							<div id="center">
								<h:outputText value="#{pageHint}" rendered="#{not empty pageHint}" />
								<h2 class="contentheading"><h:outputText value="#{pageHeading}" /></h2>
								<ui:insert name="content">Main Content</ui:insert>
							</div>
						</h:panelGroup>
					</div>
				</h:panelGroup>
				<div class="push"></div>
				<h:panelGroup id="footer" layout="block">
					<div class="inner">
						<h:form id="footerForm">
							<div class="moduletable">
								<p>
									<h:commandLink action="#{navigationHelper.getFooterTermsOfUse}" value="#{messages.footer_termsofuse}"></h:commandLink>
								</p>
							</div>
							<div class="moduletable">
								<p>
									<h:commandLink action="#{navigationHelper.getFooterContact}" value="#{messages.footer_contact}"></h:commandLink>
								</p>
							</div>
							<div class="moduletable">
								<p>
									<h:commandLink action="#{navigationHelper.getFooterLegalNotice}" value="#{messages.footer_legalnotice}"></h:commandLink>
								</p>
							</div>
							
							<div class="version">
								<p>
									<h:outputText value="V#{applicationConfiguration.version}.r#{applicationConfiguration.buildNumber} Env:#{applicationConfiguration.environment}"></h:outputText>
								</p>
							</div>	
						</h:form>
					</div>
				</h:panelGroup>
			</h:panelGroup>
		</div>
	</div>
	</h:body>
</f:view>
</html>

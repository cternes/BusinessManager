<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">

	<p:panel header="#{messages.spoken_languages}">
		<h:dataTable var="languageBean" value="#{languageController.languageBeans}">
			<h:column>
				<h:outputLabel value="#{languageBean.text}:" />
			</h:column>

			<h:column>
				<h:selectBooleanCheckbox value="#{languageBean.isChecked}" />
			</h:column>
		</h:dataTable>
	</p:panel>

</ui:composition>